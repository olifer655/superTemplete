.sidebar {
  width: 200px;
  overflow: hidden;
  padding: 10px;
  border: 1px solid #dcdcdc;
  height: 90vh;
  text-align: center;
  & .title {
    color: red;
  }
}

.moudel {
  border: 1px solid #dcdcdc;
  cursor: move;
  height: 35px;
  line-height: 35px;
  box-shadow: 0px 1px 1px #ECECEC;
  background: #fff;
  margin-bottom: 10px;
}

.moudelName {
  display: inline-block;
  width: 70%;
}
